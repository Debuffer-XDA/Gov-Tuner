busybox="/system/etc/GovTuner/busybox"
ver="4.0 - RC 23.1"
divider="==============================================="
$busybox echo "CHANGELOG FOR $ver"
$busybox echo "----------------------"
$busybox echo ""
$busybox echo "Now supports more devices! (mostly cluster devices)"
$busybox echo "New algorithm in detecting clusters!"
$busybox echo "Tuned Parameters updated"
$busybox echo "Tweaked Parameters added"
$busybox echo "Fixed On boot error"
$busybox echo "No more gt_test! All GovTuner init is now in gt_init"
$busybox echo "Customizable sleep time in gt_init"
$busybox echo "SuperSU su.d support"
$busybox echo "Increase Performance Added"
$busybox echo "New Parameters for ondemand/conservative based governors"
$busybox echo "Fixed Several issues with several devices
(S7 Edge, Mi 4i, Mi 4c?, Note 4, Lenovo P1 Turbo, Zenfone 2 and etc)"
$busybox echo "Governor Changer Updated"
$busybox echo "Added option to change Execution Time Interval"
$busybox echo "Some Optimizations for a few governor parameters"
$busybox echo "Removed unsupported parameters for certain governors"
$busybox echo "First launch since boot detector added"
$busybox echo "Almost all saved Choices are saved"
$busybox echo "Profiles' code reduced"
$busybox echo "New UI with fancy animations"
$busybox echo "Optimized Code"
$busybox echo ""
$busybox echo ""
$busybox echo -n "Press enter to go back: "
read
clear
exit
