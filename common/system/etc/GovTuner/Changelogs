busybox="/system/etc/GovTuner/busybox"
ver="3.7"
divider="==============================================="
$busybox echo "CHANGELOG FOR $ver"
$busybox echo "----------------------"
$busybox echo ""
$busybox echo "Battery profile improvements"
$busybox echo "Balanced profile improvements"
$busybox echo "Performance profile improvements"
$busybox echo "Added Zzmoove governor compatibility"
$busybox echo "Added Zzmoove built-in hotplug compatibility"
$busybox echo "Added more options to user in the main screen"
$busybox echo "Added Zzmoove/Stock hotplug apply for zzmoove governor users"
$busybox echo ""
$busybox echo -n "b. Back to menu: "
read -r character
case $character in
b|B)$busybox clear;
sh /system/bin/govtuner;;
esac
